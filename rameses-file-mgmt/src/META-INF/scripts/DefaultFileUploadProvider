import com.rameses.rcp.annotations.*;
import com.rameses.rcp.common.*;
import com.rameses.filemgmt.*;

public class DefaultFileUploadProvider implements FileUploadProvider {
    
    List getLocations() { 
        return null; 
    } 

    Map save( Map o ) { 
        println '** saving -> '+ o;  
        return o; 
    } 

    void ontransfer( Map o, Number bytestransferred ) { 
        println '** ontransfer: '+ o.fileid + ' ('+ bytestransferred +')'; 
    } 

    void oncomplete( Map o ) {
        println '** oncomplete '+ o.fileid;
    }
} 
